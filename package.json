{
  "name": "deojs",
  "version": "1.1.1",
  "description": "DeoJS allows easy structurization of your express app.",
  "main": "src/index.js",
  "scripts": {
    "dev": "tsc && node example/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": ["web-framework"],
  "author": "FishingHacks",
  "license": "ISC",
  "dependencies": {
    "@jest/globals": "^29.4.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "jest": "^29.4.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
